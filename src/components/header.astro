---
import Link from "../components/common/link.astro";

const routes: { href: string; text: string }[] = [
  { href: "/", text: "ednoesco" },
  { href: "/blogs", text: "blogs" },
  { href: "/gpg", text: "gpg" },
];
---

<header class="mb-10 flex justify-end py-2">
  {
    routes.map(({ href, text }) => (
      <Link href={href} class="pl-5">
        {text}
      </Link>
    ))
  }
</header>
