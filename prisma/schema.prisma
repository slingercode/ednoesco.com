generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

datasource db {
  provider             = "mysql"
  url                  = env("PLANETSCALE_DATABASE_URL")
  referentialIntegrity = "prisma"
}

model meenreferencia {
  id    String @id @db.VarChar(25)
  count Int
}
